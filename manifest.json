{
  "manifest_version": 3,
  "name": "Jira Board Width Updater (Beta)",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://*.atlassian.net/jira/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  }
}